[% USE String.Compare %]
    <nav class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
        <div class="navbar-header">
          <span><a class="navbar-brand" href="/"><b>BalanceOfPower</b></a></span>
        </div>
      </div><!-- /.container -->
    </nav><!-- /.navbar -->


    <div class="container">
    <div class="page-header">
        <center><h2><a href="/world/[% game %]/g/years">[% game %]</a></h2></center>
        [% IF year and turn %]
            <p><center>
            [% this_turn = "$year/$turn" %]
            [% IF year AND turn %]
            [% IF first_year != this_turn %]
            <a href="/world/[% game %]/[% first_year %]/g/newspaper">[% first_year %]</a>
            [% END %]
            [% IF prev_turn %]
                <a href="/world/[% game %]/[% prev_turn %]/g/newspaper">&lt;</a>
            [% END %]
                <a style="font-size:25px" href="/world/[% game %]/[% year %]/[% turn %]/g/newspaper"><b>[% this_turn %]</b></a>
            [% IF next_turn %]
                <a href="/world/[% game %]/[% next_turn %]/g/newspaper">&gt;</a>
            [% END %]
            [% END %]
            [% IF current_year != this_turn %]
                <a href="/world/[% game %]/[% current_year %]/g/newspaper">[% current_year %]</a>
            [% END %]
            </center><p>
        [% END %]
        </div>
        [% INCLUDE blocks/alerts.tt %]

      <div class="row row-offcanvas row-offcanvas-right">

